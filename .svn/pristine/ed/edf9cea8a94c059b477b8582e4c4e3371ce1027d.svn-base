export const deleteData = async (url) => {
  try {
    const response = await fetch(url, {
      method: "DELETE",
    });

    if (response.ok) {
      return true;
    } else {
      throw new Error("Failed to delete data");
    }
  } catch (error) {
    console.error("Error deleting data:", error);
    throw error;
  }
};
